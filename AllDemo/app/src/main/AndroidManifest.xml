<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alldemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.alldemo.DashboardActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.animation.MainActivity"
            android:label="@string/app_name"
            />

        <activity
            android:name="com.animation.Main2Activity"
            android:label="@string/app_name"
            />

        <activity
            android:name="com.animation.Main3Activity"
            android:label="@string/app_name"
            />

        <activity
            android:name="com.animation.Main4Activity"
            android:label="@string/app_name"
            />

        <activity
            android:name="com.animation.Main5Activity"
            android:label="@string/app_name"
            />

    </application>

</manifest>